@{

}

<div class ="container">
    <h1>Cuentas</h1>
    <br />
    <a href="/cuenta/create">Crear Cuenta</a>
    <table class="table">
        <thead>
            <tr>
                <th>Cuenta</th>
                <td>Categoria</td>
                <td>Saldo</td>
                <td>Moneda</td>
                <td>acciones</td>
            </tr>
        </thead> 
        <tbody>
            @foreach(var item in ViewBag.listacuentas)
            {
                <tr>
                    <td>@item.nombre</td>    
                    <td>@item.categoria</td>  
                    <td>@item.saldoini</td>    
                    <td>@item.moneda</td>
                    <td>
                        <a class="" href="">Nuevo Ingreso</a>
                        <a class="" href="">Nuevo Gasto</a>
                        <a class="" href="">Lista Ingresos/Gastos</a>
                    </td>
                </tr>
            }
        </tbody>

    </table>
    
</div>